{
  "framework": "nextjs",
  "installCommand": "npm install && pip install -r requirements.txt",
  "buildCommand": "next build",
  "functions": {
    "api/rank.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}